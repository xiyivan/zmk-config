#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/outputs.h>
#include <dt-bindings/zmk/mouse.h>

#define DEFAULT 0
#define LOWER 1


/ {
    keymap {
        compatible = "zmk,keymap";

                default_layer {
            bindings = <
                &kp ESC  &kp Q  &kp W  &kp E  &kp R  &kp T  &kp Y  &kp U  &kp I  &kp O  &kp P  &kp BSLH
                
                &kp TAB  &kp A  &kp S  &kp D  &kp F  &kp G  &kp H  &kp J  &kp K  &kp L  &kp SEMI  &kp SQT
                
                &kp FSLH  &kp Z  &kp X  &kp C  &kp V  &kp B  &kp N  &kp M  &kp COMMA  &kp DOT
                
                &kp LBKT  &kp RBKT  &kp MINUS  &kp EQUAL
                
                &kp ESC &kp TAB &kp BSPC &kp DEL

                &kp LALT, &kp LGUI, &kp RLAT, &kp ENTER

                &kp LSFT &kp SPACE &kp SPACE &kp RSFT
            >;
        };

        lower_layer {
            bindings = <
                &kp F1  &kp F2  &kp F3  &kp F4  &kp F5  &kp F6  &kp F7  &kp F8  &kp F9  &kp F10  &kp F11 &kp F12
                
                &trans  &kp N1  &kp N2  &kp N3  &kp N4  &kp N5  &kp N6  &kp N7  &kp N8  &kp N9  &kp N0  &trans
                
                &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans
                
                &bt BT_SEL 0  &bt BT_SEL 1  &bt BT_CLR  &bootloader
                
                &trans  &trans  &trans  &trans  
                &trans  &trans  &trans  &trans 
                &trans  &trans  &trans  &trans
            >;
        };

    };
};
